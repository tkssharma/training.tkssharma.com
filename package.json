{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "http-proxy-middleware": "^0.20.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "request": "^2.88.0",
    "sass-lint": "^1.13.1",
    "workbox-sw": "^4.3.1",
    "workbox-webpack-plugin": "^4.3.1",
    "graphql": "^14.4.2",
    "apollo-boost": "^0.4.3",
    "apollo-link-dedup": "^1.0.20",
    "apollo-link-error": "^1.1.11",
    "react-apollo": "^2.5.8",
    "react-apollo-network-status": "^2.0.0-alpha.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-server": "source env.sh && node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sasslint": "sass-lint 'src/**/**/*.s+(a|c)ss' -v --no-exit",
    "sasslint:fix": "sass-lint-auto-fix",
    "lint": "./node_modules/.bin/eslint .  --ext .js",
    "e2e": "cypress open",
    "delete:reports": "rm cypress/results/* || true",
    "prereport": "npm run delete:reports",
    "report": "cypress run || true",
    "postreport": "mochawesome-merge --reportDir cypress/results > cypress/results/index.json && marge cypress/results/index.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "mocha": "5.2.0",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.0.1",
    "mochawesome-report-generator": "^4.0.1",
    "sonar-scanner": "^3.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0"
  }
}
